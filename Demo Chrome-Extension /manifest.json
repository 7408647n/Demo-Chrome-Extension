{
  "name": "Demo",
  "description": "A simple Demo Chrome-Extension!",
  "version": "0.1",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_title": "Demo",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "64": "images/icon64.png",
      "128": "images/icon128.png"
    }
  },
  "options_ui": {
    "page": "extension-options.html",
    "open_in_tab": false
  },
  "background": {
    "service_worker": "extension-background.js"
  }
}